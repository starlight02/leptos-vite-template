{
  "name": "leptos-vite-app",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"pnpm run wasm:watch\" \"vite\" --names \"WASM,VITE\" --prefix-colors \"cyan,green\"",
    "wasm:watch": "cargo watch -i pkg -i target -w src -w Cargo.toml -w Cargo.lock -s \"wasm-pack build --target web --no-typescript --dev\"",
    "wasm:build": "wasm-pack build --target web --no-typescript --release",
    "build": "pnpm run wasm:build && vite build",
    "build:analyze": "pnpm run build && node scripts/analyze-bundle.js",
    "preview": "vite preview",
    "clean": "rimraf pkg dist target/wasm32-unknown-unknown"
  },
  "devDependencies": {
    "@types/node": "^24.0.15",
    "concurrently": "^9.2.0",
    "rimraf": "^6.0.0",
    "rollup-plugin-visualizer": "^6.0.3",
    "typescript": "^5.8.3",
    "vite": "^7.0.5",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-top-level-await": "^1.6.0",
    "vite-plugin-wasm": "^3.5.0"
  },
  "dependencies": {
    "mdui": "^2.1.4"
  },
  "web-types": [
    "./node_modules/mdui/web-types.zh-cn.json"
  ]
}
